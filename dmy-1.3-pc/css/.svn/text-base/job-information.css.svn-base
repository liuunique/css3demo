}
.position p{
	font-size: 14px;
	line-height: 30px;
	color: #333333;
	float: left;
	margin-right: 20px;
}
.position p:first-child{
	font-size: 18px;
	color: #3479CA;
}
.information2{
	width: 730px;
	height: 30px;
}
.information2 p{
	color: #999999;
